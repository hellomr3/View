<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="OperateSeekBar">
        <attr name="seekBar_title" format="string" />
        <attr name="seekBar_titleColor" format="color" />
        <attr name="seekBar_TintColor" format="color" />
        <attr name="seekBar_maxProgress" format="integer" />
        <attr name="seekBar_progress" format="integer" />
    </declare-styleable>

    <!--可折叠展开的TextView-->
    <declare-styleable name="FoldTextView">
        <!--默认展示多少行-->
        <attr name="fold_showLines" format="integer" />
        <!--内容-->
        <attr name="fold_content" format="string" />
        <!--Btn-->
        <attr name="fold_foldText" format="string" />
        <attr name="fold_flatText" format="string" />
        <attr name="fold_btnTextColor" format="color" />
        <attr name="fold_btnTextSize" format="dimension" />
    </declare-styleable>

    <!--时间线 Timeline-->
    <declare-styleable name="Timeline">
        <attr name="middle_line_color" format="color" />

        <attr name="time_line_color" format="color" />
        <attr name="time_line_height" format="dimension" />
        <attr name="time_line_width" format="dimension" />

        <attr name="usable_area_color" format="color" />

        <attr name="android:textSize" format="dimension" />
        <attr name="android:textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ScalableImageView">
        <attr name="android:src" format="reference" />
    </declare-styleable>

    <declare-styleable name="FriendLayout">
        <attr name="normalMargin" format="dimension" />
    </declare-styleable>

    <!--RecordTimer-->
    <declare-styleable name="RecordTimer">
        <attr name="defaultTimeSecond" format="integer" />
    </declare-styleable>

    <!--BlockButton-->
    <declare-styleable name="BlockButton">
        <attr name="leadIcon" format="reference" />
        <!--title-->
        <attr name="leadTitle" format="string" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="titleTextColor" format="color" />
        <!--desc-->
        <attr name="tailDesc" format="string" />
        <attr name="descTextSize" format="dimension" />
        <attr name="descTextColor" format="color" />
        <attr name="tailIcon" format="reference" />
        <!--SeparatorLine-->
        <attr name="showSeparatorLine">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="1" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="2" />
        </attr>
        <attr name="separatorLineColor" format="color" />
    </declare-styleable>
</resources>