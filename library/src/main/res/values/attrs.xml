<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="OperateSeekBar">
        <attr name="seekBar_title" format="string" />
        <attr name="seekBar_titleColor" format="color" />
        <attr name="seekBar_TintColor" format="color" />
        <attr name="seekBar_maxProgress" format="integer" />
        <attr name="seekBar_progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="FoldTextView">
        <!--默认展示多少行-->
        <attr name="fold_showLines" format="integer" />
        <!--内容-->
        <attr name="fold_content" format="string" />
        <attr name="fold_contentColor" format="color" />
        <attr name="fold_contentSize" format="dimension" />
        <!--Btn-->
        <attr name="fold_btnText" format="string" />
        <attr name="fold_btnTextColor" format="color" />
        <attr name="fold_btnTextSize" format="dimension" />
        <attr name="fold_foldIcon" format="reference" />
        <attr name="fold_unFoldIcon" format="reference" />
    </declare-styleable>

    <!--时间线 Timeline-->
    <declare-styleable name="Timeline">
        <attr name="middle_line_color" format="color" />

        <attr name="time_line_color" format="color" />
        <attr name="time_line_height" format="dimension" />
        <attr name="time_line_width" format="dimension" />

        <attr name="usable_area_color" format="color" />

        <attr name="android:textSize" format="dimension" />
        <attr name="android:textColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ScalableImageView">
        <attr name="android:src" format="reference" />
    </declare-styleable>

    <declare-styleable name="FriendLayout">
        <attr name="normalMargin" format="dimension" />
    </declare-styleable>
</resources>